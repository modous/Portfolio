<%- include('./partials/head') %>
<section class="work-page-container">
  <a class="goback-btn" href="/goback">Go Back</a>
  <section class="image-container">
    <article>
      <h1 class="h1-work">
        These are some of my school and work projects, the rest you can find in
        my
        <a class="Github-link" href="https://github.com/modous" target="_blank"
          >Github!</a
        >
      </h1>
    </article>

    <% projects.forEach(function(project) { %>
    <article class="work-article">
      <h2 class="work-title"><%= project.title %></h2>
      <a href="<%= project.url %>" target="_blank">
        <img class="work" src="<%= project.image.url %>" alt="" />
      </a>
    </article>
    <% }); %>
  </section>
</section>
